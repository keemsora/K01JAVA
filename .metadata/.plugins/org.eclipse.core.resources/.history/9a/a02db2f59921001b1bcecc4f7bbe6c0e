package quiz;

public class BubbleSort {
	
	private static void swap() {
		
		int num = (int)(Math.random()*100)+1;
		
		int[] arr = new int[num];	
		
		while(true) {
			for(int i=0; i<arr.length; i++) {
				arr[i] = num;
				System.out.print(arr[i]+"");
			}
		}

	}
	public static void main(String[] args) {
		
		
		swap();
		
	}
}
