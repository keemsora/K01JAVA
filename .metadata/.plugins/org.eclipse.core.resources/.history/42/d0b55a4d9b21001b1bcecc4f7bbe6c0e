package quiz;

public class BubbleSort {
	
	private static void swap() {

		
		int[] arr = new int[10];//크기가 10인 배열 생성
		int temp;
		
		for(int i=0; i<arr.length; i++) {
				int num = (int)(Math.random()*100)+1;
				arr[i] = num;
				System.out.print(arr[i]+" ");
		}//배열 초기화
		
		for(int i=0; i<arr.length; i++) {
			if(arr[i]>arr[i+1]) {
				temp = arr[i];
				arr[i] = arr[i+1];
				arr[i+1] = temp;
			}
			else {
				break;
			}
		}

		
		

	}
	public static void main(String[] args) {
		
		
		swap();
		
	}
}
